{
    "name": "gerpayt/yii2-alipay",
    "description": "Alipay for Yii2.",
    "keywords": [
        "yii",
        "extension",
        "alipay"
    ],
    "type": "yii2-extension",
    "license": "Apache",
    "authors": [
        {
            "name": "Chen Feng",
            "email": "gerpayt@qq.com",
            "homepage": "http://blog.gerpayt.cn"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "gerpayt/yii2-alipay",
                "version": "dev-master",
                "source": {
                    "url": "git://github.com/gerpayt/yii2-alipay.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "gerpayt\\yii2_alipay\\" : ""
        }
    }
}
